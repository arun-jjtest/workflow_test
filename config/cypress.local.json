{
    "viewportHeight": 800,
    "viewportWidth": 1200,
    "baseUrl": "http://localhost:3000/",
    "video": false,
    "reporter": "cypress-mochawesome-reporter",
    "reporterOptions": {
        "reportDir": "cypress/report",
        "charts": true,
        "embeddedScreenshots": true
    },
    "pageLoadTimeout": 10000,
    "responseTimeout": 10000,
    "defaultCommandTimeout": 10000,
    "retries": { 
        "runMode": 1,
        "openMode": 1
    }
}